s/toshihito_endo/Desktop/React/food-order-app/src/components/Layout/HeaderCartButton.js",["94"],[],"import React from \"react\";\nimport CartIcon from \"../Cart/CartIcon\";\nimport { useContext, useEffect, useState } from \"react\";\n\nimport classes from './HeaderCartButton.module.css';\nimport CartContext from \"../../store/cart-context\";\n\nconst HeaderCartButton = props => {\n    const [btnIsHighlighted, setBtnIsHighlighted] = useState(false);\n    const cartCtx = useContext(CartContext);\n\n    const { items } = cartCtx;\n\n    const numberOfCartItems = items.reduce((curNumber, item) => {\n        return curNumber + item.amount;\n    }, 0);\n\n    const btnClasses = `${classes.button} ${btnIsHighlighted ? classes.bump : ''}`;\n\n    useEffect(() => {\n        if (cartCtx.items.length === 0) { return; }\n\n        setBtnIsHighlighted(true);\n\n        const timer = setTimeout(() => { setBtnIsHighlighted(false); }, 300);\n\n        return () => {\n          clearTimeout(timer);  \n        };\n    }, [items]);\n\n    return (\n        <button className={btnClasses} onClick={props.onClick}>\n            <span className={classes.icon}>\n                <CartIcon />\n            </span>\n            <span>Your Cart</span>\n            <span className={classes.badge}>{numberOfCartItems}</span>\n        </button>\n    );    \n};\n\nexport default HeaderCartButton;\n",[],"/Users/toshihito_endo/Desktop/React/food-order-app/src/components/Cart/CartIcon.js",[],[],"/Users/toshihito_endo/Desktop/React/food-order-app/src/components/Meals/Meals.js",[],[],"/Users/toshihito_endo/Desktop/React/food-order-app/src/components/Meals/MealsSummary.js",[],[],"/Users/toshihito_endo/Desktop/React/food-order-app/src/components/Meals/AvailableMeals.js",["95"],[],"/Users/toshihito_endo/Desktop/React/food-order-app/src/components/UI/Card.js",[],[],"/Users/toshihito_endo/Desktop/React/food-order-app/src/components/Meals/MealItem/MealItem.js",[],[],"/Users/toshihito_endo/Desktop/React/food-ord